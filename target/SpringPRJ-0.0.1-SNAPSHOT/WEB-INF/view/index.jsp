<!-- SLIDER -->
<section class="slider d-flex align-items-center" style="background-image:url(../../img/background_pastafork.jpg);">
	<div class="container">
		<div class="row d-flex justify-content-center">
			<div class="col-md-12">
				<div class="slider-title_box">
					<div class="row">
						<div class="col-md-12">
							<div class="slider-content_wrap">
								<h1 style="font-family: 'Sunflower'">서울의 인기 식당들을 찾아보세요</h1>
								<h5 style="font-family: 'Sunflower'">서울에서 먹고, 마시고, 즐길 수 있는 식당들을 찾아봅시다!</h5>
							</div>
						</div>
					</div>
					<div class="row d-flex justify-content-center">
						<div class="col-md-10">
							<form class="form-wrap mt-4" action="searchlist.do">
								<label class="radio-inline" style="color: #fff; font-size: 18px; font-weight: bold;">
									<input type="radio" name="type" id="name" value="상호" checked> 상호
								</label>
								<label class="radio-inline" style="color: #fff; font-size: 18px; font-weight: bold;">
									<input type="radio" name="type" id="cat" value="카테고리"> 카테고리
								</label>

								<div class="btn-group" role="group" aria-label="Basic example">
									<label for="place"></label><input type="text" autocomplete="off" placeholder="검색어를 입력하세요" class="btn-group2" id="place" style="width: 80%;" name="SearchQuery" required>
									<button type="submit" class="btn-form"><span class="icon-magnifier search-icon"></span>검색<i class="pe-7s-angle-right"></i></button>
								</div>
							</form>
							<div class="slider-link">
								<div style="cursor: pointer;"><a onclick="return cat(6);">최근 추가 된 식당</a></div>
							</div>
							<script src="https://static.landbot.io/landbot-widget/landbot-widget-1.0.0.js"></script>
							<script>
								var myLandbot = new LandbotPopup({
									index: 'https://landbot.io/u/H-322359-HWGIY1P38E7NBBP1/index.html',
								});
							</script>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

<!--// SLIDER -->
<!--//END HEADER -->

<!--============================= FEATURED PLACES =============================-->
<section class="main-block light-bg">
	<div class="container">
		<div class="row justify-content-center">
			<div class="col-md-5">
				<div class="styled-heading">
					<h3 style="font-family: 'Sunflower'">Featured Places</h3>
				</div>
			</div>
		</div>
		<div class="row">
			<%for (int i = 0; i<3; i++) { %>
				<div class="col-md-4 featured-responsive">
					<div class="featured-place-wrap">
						<a href="listDetail.do?res_name=<%=lList.get(i).getRes_name()%>">
							<img src='/img/<%=lList.get(i).getRes_name()%>.jpg' class="img-fluid" alt="#">
							<div class="featured-title-box">
								<h6><%=lList.get(i).getRes_name()%></h6>
								<p class="long-category"><%=lList.get(i).getRes_cat()%></p> <span>• </span>
								<p><span><%=lList.get(i).getRes_price()%></span></p>
								<ul>
									<li><span class="icon-location-pin"></span>
										<p class=long-text"><%=lList.get(i).getRes_add()%></p>
									</li>
									<li><span class="icon-screen-smartphone"></span>
										<p class="long-text"><%=lList.get(i).getRes_tel()%></p>
									</li>
									<li><span class="icon-link"></span>
										<p class="long-text"><%=lList.get(i).getRes_time()%></p>
									</li>

								</ul>
							</div>
						</a>
					</div>
				</div>
			<%} %>
		</div>
		<div class="row justify-content-center">
			<div class="col-md-4">
				<div class="featured-btn-wrap">
					<a href="featuredlist.do" class="btn btn-danger">더보기</a>
				</div>
			</div>
		</div>
	</div>
</section>
